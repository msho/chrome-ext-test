{
  "name": "GRSEE Zoho Calendar Sync",
  "version": "1.0",
  "description": "Click to sync Zoho task to calendar",
  "content_scripts": [
    {
      "matches": [
        "https://projects.zoho.com/portal/grseeconsulting*"
      ],
      "js": [
        "dom/main.js"
      ]
    }
  ],
  "permissions": [
    "identity",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background/main.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}